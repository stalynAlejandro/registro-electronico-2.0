{
    "name": "registro-electronico",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:mock": "concurrently --kill-others \"npm run mock:api\" \"npm run dev\"",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "dev-https": "set HTTPS=true&&npm dev",
        "mock:api": "json-server --watch src/mock/db.cjs --port 3010 --routes src/mock/routes.json --middlewares src/mock/middleware.cjs --delay 500 --static src/mock/files",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@emotion/react": "^11.10.8",
        "@emotion/styled": "^11.10.8",
        "@mui/icons-material": "^5.11.11",
        "@mui/material": "^5.12.2",
        "@mui/styled-engine-sc": "^5.11.11",
        "@mui/system": "^5.11.13",
        "@reduxjs/toolkit": "^1.9.3",
        "amplitudejs": "^5.3.2",
        "i18next": "^22.4.13",
        "i18next-browser-languagedetector": "^7.0.1",
        "i18next-http-backend": "^2.2.0",
        "json-server": "^0.17.2",
        "lodash": "^4.17.21",
        "match-sorter": "^6.3.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^12.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.10.0",
        "react-toastify": "^9.1.1",
        "redux-persist": "^6.0.0",
        "sort-by": "^0.0.2",
        "styled-components": "^5.3.9",
        "vite-plugin-svgr": "^2.4.0",
        "yup": "^1.0.2"
    },
    "devDependencies": {
        "@redux-devtools/core": "^3.13.1",
        "@storybook/addon-actions": "^7.0.2",
        "@storybook/addon-controls": "^7.0.2",
        "@storybook/addon-essentials": "^7.0.2",
        "@storybook/addon-interactions": "^7.0.2",
        "@storybook/addon-links": "^7.0.2",
        "@storybook/blocks": "^7.0.2",
        "@storybook/jest": "^0.1.0",
        "@storybook/react": "^7.0.2",
        "@storybook/react-vite": "^7.0.2",
        "@storybook/testing-library": "^0.1.0",
        "@testing-library/react": "^14.0.0",
        "@types/amplitude-js": "^8.16.2",
        "@types/lodash": "^4.14.192",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@vitejs/plugin-react": "^3.1.0",
        "@vitest/ui": "^0.30.1",
        "file-loader": "^6.2.0",
        "jsdom": "^21.1.1",
        "prop-types": "^15.8.1",
        "storybook": "^7.0.2",
        "typescript": "^4.9.3",
        "vite": "^4.2.0",
        "vitest": "^0.29.8"
    }
}